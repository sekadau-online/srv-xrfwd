# Server Configuration
SERVER_HOST=0.0.0.0
SERVER_PORT=2222
WEB_INTERFACE=127.0.0.1
WEB_PORT=8080

# SSH Configuration
SSH_AUTHORIZED_KEYS_PATH=/home/user/.ssh/authorized_keys
SSH_SERVER_KEY_PATH=/etc/ssh/ssh_host_rsa_key

# Forwarding Configuration
ALLOWED_PORTS=3000,8080,9000
MAX_CONNECTIONS=10
SESSION_TIMEOUT=3600

# Security
ALLOWED_USERS=user1,user2
REQUIRE_PUBKEY_AUTH=true
